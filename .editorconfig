# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true # remove todos espa√ßos em branco
insert_final_newline = true #  vai sempre adicionar uma linha vazia no final do meu arquivo.

